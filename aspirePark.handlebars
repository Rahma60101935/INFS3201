
<head>

  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Cat Feeding Site</title>
  <style>
    body {
      font-family: Arial, sans-serif;
      margin: 0;
      padding: 0;
      background-color: #f4f4f4;
    }

    .container {
      max-width: 800px;
      margin: 20px auto;
      padding: 20px;
      background-color: #fff;
      border-radius: 5px;
      box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
    }

    h1 {
      text-align: center;
    }

    table {
      width: 100%;
      border-collapse: collapse;
      margin-bottom: 20px;
    }

    table th,
    table td {
      padding: 10px;
      text-align: left;
      border-bottom: 1px solid #ddd;
    }

    .report-issue {
      background-color: #f9f9f9;
      padding: 20px;
      border-radius: 5px;
    }

    .report-issue h2 {
      margin-top: 0;
    }
  </style>
</head>
<body>
  <div class="container">
    <h1>Cat Feeding Site</h1>
    <table>
                <tr>
                  <th>Location</th>
                    <th>Latitude</th>
                    <th>Longitude</th>
                    <th>Description</th>
                    <th>Last Visited</th>
                    <th>Water Status</th>
                    <th>Food Status</th>
                    <th>Food Provided</th>
                    <th>Water Provided</th>
                     <th>Cat Count</th>
                
                </tr>
                
                {{#each station}}
                <tr>
                    
                      
                    <td>{{this.location}}</td>
                    <td>{{this.latitude}}</td>
                    <td>{{this.longitude}}</td>
                    <td>{{this.description}}</td>
                    <td>{{this.last_visited}}</td>
                    <td>{{this.water_status}}</td>
                    <td>{{this.food_status}}</td>
                    <td>{{this.food_provided}}</td>
                    <td>{{this.water_provided}}</td>
                    <td>{{this.cats_count}}</td>
                   
                  
                    
                  
                </tr>
                
                {{/each}}
            </table>
    <div class="report-issue">
      <h2>Report Issue</h2>
       {{#each station}}
     {{this.reportIssue}} 
          {{/each}}
    </div>
  </div>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/handlebars.js/4.7.7/handlebars.min.js"></script>
  <script src="scripts.js"></script>
</body>
</html>
