<html>
  <h1>Cat Feeding Site</h1>
    <table>
                <tr>
                  <th>Location</th>
                    <th>Latitude</th>
                    <th>Longitude</th>
                    <th>Description</th>
                    <th>Last Visited</th>
                    <th>Water Status</th>
                    <th>Food Status</th>
                    <th>Food Provided</th>
                    <th>Water Provided</th>
                     <th>Cat Count</th>
                
                </tr>
                
                {{#each station}}
                <tr>
                    
                      
                    <td>{{this.location}}</td>
                    <td>{{this.latitude}}</td>
                    <td>{{this.longitude}}</td>
                    <td>{{this.description}}</td>
                    <td>{{this.last_visited}}</td>
                    <td>{{this.water_status}}</td>
                    <td>{{this.food_status}}</td>
                    <td>{{this.food_provided}}</td>
                    <td>{{this.water_provided}}</td>
                    <td>{{this.cats_count}}</td>
                   
                  
                    
                  
                </tr>
                
                {{/each}}
            </table>
   <h2>Post made by Members</h2>
    <div class="other-images">
        {{#each images}}
            <div class="image-container">
                <p class="image-text">{{this.posts}}</p>
                <img src="/static/{{this.fileName}}" alt="Image">
            </div>
        {{/each}}
    </div>
</html>